<root>
    <styles>
        <include src="s2r://panorama/styles/dotastyles.vcss_c" />
        <include src="s2r://panorama/styles/custom_game/fateanother_customization.vcss_c" />
    </styles>
    <scripts>
        <include src="s2r://panorama/scripts/custom_game/fateanother_util.vjs_c" /> 
        <include src="s2r://panorama/scripts/custom_game/fateanother_customization.vjs_c" /> 
        <include src="s2r://panorama/scripts/sounds.vjs_c" />
    </scripts>
    <Panel class="CustomizationBoardRoot" hittest="false">
		<Panel id="CustomizeServant" hittest="false">
		<!--<Image src="s2r://panorama/images/misc/customize_png.vtex"/>-->
		
	    <Panel id="CustomizationBoard" hittest="false" >
	    	<Panel id="CustomizationBoardLabel" hittest="false">
				<Label id="CustomizeBoardLabelText" text="#Fateanother_Customize" hittest="false"/>
			</Panel>
			<Panel id="CustomizationStat1" hittest="false"/>
			<Panel id="CustomizationStat2" hittest="false"/>
			<Panel id="CustomizationStat3" hittest="false"/>
			<Panel id="CustomizationTopContainer" hittest="false">
				<Panel id="CustomizationAttributePanel" hittest="false">
					<Label id="CustomizationAttributeText" class="CustomizationText" text="#Fateanother_Customize_Attributes" onmouseover="AttributeShowTooltip()" onmouseout="AttributeHideTooltip()" hittest="true" />
					<Panel id="CustomizationAttributeLayout" hittest="false">
					</Panel>
				</Panel>
	            <Panel id="CustomizationShardPanel" hittest="false">
	            	<Panel id="CustomizationShardTextLayout" hittest="false">
		            	<Label id="CustomizationShardText" class="CustomizationText" text="#Fateanother_Customize_Special_Shards" onmouseover="ShardShowTooltip()" onmouseout="ShardHideTooltip()" hittest="true" />
		            	<Label id="CustomizationShardNumber" text="0" hittest="true"/>
		            </Panel>
	            	<Panel id="CustomizationShardLayout" hittest="false">
		            </Panel>
	            </Panel>
			</Panel>
			<Panel id="CustomizationBottomContainer" hittest="false">
				<Panel id="CustomizationComboPanel" hittest="false">
					<Label id="CustomizationComboText" class="CustomizationText" text="#Fateanother_Customize_Special_Cooldowns" onmouseover="ComboShowTooltip()" onmouseout="ComboHideTooltip()" hittest="true" />
					<Panel id="CustomizationCooldownLayout" hittest="false">
					</Panel>
				</Panel>
				<Panel id="CustomizationStatPanel" hittest="false">
					<Label id="CustomizationStatText" class="CustomizationText" text="#Fateanother_Customize_Stats" onmouseover="StatShowTooltip()" onmouseout="StatHideTooltip()" hittest="true"/>
					<Panel id="CustomizationStatLayout" hittest="false">
					</Panel>
					<Panel id="CustomizationStatAmountLayout" hittest="false">
						<Panel class="StatAmountContainer">
							<Label id="STRAmount" class="StatDisplayText" text="0 / 50" hittest="false"/>
						</Panel>
						<Panel class="StatAmountContainer">
							<Label id="AGIAmount" class="StatDisplayText" text="0 / 50" hittest="false"/>
						</Panel>
						<Panel class="StatAmountContainer">
							<Label id="INTAmount" class="StatDisplayText" text="0 / 50" hittest="false"/>
						</Panel>
						<Panel class="StatAmountContainer">
							<Label id="DMGAmount" class="StatDisplayText" text="0 / 50" hittest="false"/>
						</Panel>
						<Panel class="StatAmountContainer">
							<Label id="ARMORAmount" class="StatDisplayText" text="0 / 50" hittest="false"/>
						</Panel>
						<Panel class="StatAmountContainer">
							<Label id="HPREGAmount" class="StatDisplayText" text="0 / 50" hittest="false"/>
						</Panel>
						<Panel class="StatAmountContainer">
							<Label id="MPREGAmount" class="StatDisplayText" text="0 / 50" hittest="false"/>
						</Panel>
						<Panel class="StatAmountContainer">
							<Label id="MSAmount" class="StatDisplayText" text="0 / 50" hittest="false"/>
						</Panel>
						<Panel class="StatAmountContainer">
							<Label id="GoldAmount" class="StatDisplayText" text="0 / 10" hittest="false"/>
						</Panel>
					</Panel>
				</Panel>
			</Panel>
	    </Panel>
		</Panel>
        <!--<Button id="CustomizationOpenButton" onactivate="OnCustomizeButtonPressed()" onmouseover="OnCustomizeButtonShowTooltip()" onmouseout="OnCustomizeButtonHideTooltip()" hittest="true">
            <Image id="CustomizationOpenButtonImage" src="s2r://panorama/images/misc/gold_arrow_png.vtex" hittest="false"/>
            <Label id="CustomizationOpenButtonText" class="CustomizationText" text="#Fateanother_Customize" hittest="false"/>
        </Button>-->
    </Panel>

</root>