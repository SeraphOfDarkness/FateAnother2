<root>
    <styles>
        <include src="s2r://panorama/styles/dotastyles.vcss_c" />
        <include src="s2r://panorama/styles/custom_game/fateanother_event.vcss_c" />
    </styles>
    <scripts>
        <include src="s2r://panorama/scripts/custom_game/fateanother_event.vjs_c" />
    </scripts>
    <Panel class="FateEventRoot" hittest="false">
        <Panel id="EventsBar" class="EventsBar"/>
        <!--<Panel id="EventsBackground"  onactivate="OnFateEventClose()"/>-->
        <Panel id="EventMainContainer" class="EventMainContainer" hittest="false">
            <Panel id="EventCloseButton" class="EventCloseButton" onmouseactivate="OnFateEventClose()" >
                <Label id="EventCloseButtonLabel" text=" " hittest="false"/>
            </Panel>
            <Panel id="SkinDetailPanel" class="SkinDetailPanel">
                <Panel id="EventName" class="EventName">
                    <Label id="EventNameLabel" text=" " hittest="false"/>
                </Panel>
                <Panel id="EventDate" class="EventDate">
                    <Label id="EventDateLabel" text=" " hittest="false"/>
                </Panel>
            </Panel>
            <Panel id="SkinPanel" class="SkinPanel">
                <Panel id="SkinPortrait" class="SkinPortrait"/>
                <Panel id="SkinPortraitName" class="SkinPortraitName">
                    <Label id="SkinPortraitNameLabel" text=" " hittest="false"/>
                </Panel>
            </Panel>
            <Panel id="EventPanelStatus" class="EventPanelStatus">
                <Label id="EventPanelStatusLabel" text=" " hittest="false"/>
            </Panel>
        </Panel>

        <Panel id="BoardMainContainer" class="BoardMainContainer" hittest="false">
            <Panel id="EventCloseButton" class="EventCloseButton" onmouseactivate="OnFateTourClose()" >
                <Label id="EventCloseButtonLabel" text=" " hittest="false"/>
            </Panel>
            <Panel id="TourDetailPanel" class="TourDetailPanel">
                <Panel id="TourName" class="TourName">
                    <Label id="EventNameLabel" text=" " hittest="false"/>
                </Panel>
                <Panel id="EventDate" class="EventDate">
                    <Label id="EventDateLabel" text=" " hittest="false"/>
                </Panel>
            </Panel>
            <Panel id="BoardPanel" class="BoardPanel">
                <Panel id="BoardImage" class="BoardImage">

                </Panel>
                <Panel id="BoardDetail" class="BoardDetail">
                    <Label id="TourDetailLabel" text=" " hittest="false"/>
                </Panel>
                <Panel id="TourJoinPanel" class="TourJoinPanel">
                    <Panel id="JoinSolo" class="JoinButton">
                        <Label html="true" id="EventNameLabel" text=" " hittest="false"/>
                    </Panel>
                    <Panel id="JoinTeam" class="JoinButton">
                        <Label html="true" id="EventNameLabel" text=" " hittest="false"/>
                    </Panel>
                </Panel>
            </Panel>
        </Panel>
    </Panel>
</root>


